{
    "name": "laravelir/dashboarder",
    "description": "a laravel package to create dashboard admin dynamically",
    "homepage": "https://github.com/laravelir/dashboarder",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintaner",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Dashboarder\\": "src/"
        },
        "files": [
            "./src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelir\\Dashboarder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelir\\Dashboarder\\Providers\\DashboarderServiceProvider"
            ],
            "aliases": {
                "Dashboarder": "Laravelir\\Dashboarder\\Facades\\DashboarderFacade"
            }
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "license": "MIT"
}
